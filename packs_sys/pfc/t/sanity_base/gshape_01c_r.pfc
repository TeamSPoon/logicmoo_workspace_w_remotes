#!/usr/bin/env lmoo-junit

% Quick fwd test
:- include(library(logicmoo_test_header)).

:- use_module(library(statistics)).

%:- mpred_notrace_exec.
% reset runtime counter
:- statistics(runtime,_Secs).
%:- cls.

path(1,2).
path(2,3).
path(3,4).
path(1,3).
path(1,4).

~path(1,3).
~path(1,4).

:- listing(path/2).

:- mpred_test(path(3, 4)).
:- mpred_test(path(2, 3)).
:- mpred_test(path(1, 2)).
:- mpred_test(~path(1,3)).
:- mpred_test(~path(1,4)).
:- mpred_test(\+path(1,3)).
:- mpred_test(\+path(1,4)).

